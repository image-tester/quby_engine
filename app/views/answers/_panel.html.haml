%fieldset.panel
  .panel_index= panel_counter + 1

  - if panel.title
    %h1&= panel.title

  = render :partial => "item", :collection => panel.items

  .prevnext
    .next
      - if panel.next
        = button_to raw("Volgende vraag &rarr;"), :panel_id => panel.next
      - else
        = button_to "Save", questionnaire_answer_path(@questionnaire, @answer)
/ End _panel.html.haml