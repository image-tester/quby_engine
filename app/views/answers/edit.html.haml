:javascript
  var panel_validations = {}

- content_for(:page_title, @questionnaire.title)

- form_tag questionnaire_answer_path(@questionnaire, @answer), :method => :put, :class => [@questionnaire.key, session[:display_mode]].join(" ") do
  = render :partial => "answers/#{session[:display_mode]}/panel", :collection => @questionnaire.panels, :locals => {:panels => @questionnaire.panels, :questionnaire => @questionnaire}

  %fieldset.buttons
    %a(href="#" onClick="window.print(); return false" style="margin-right: 1em") Print antwoorden
    = submit_tag "Klaar"