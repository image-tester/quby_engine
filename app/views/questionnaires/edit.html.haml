%h1 
  Taking 
  = @questionnaire.key

%form{:action => answer_questionnaire_path(@questionnaire.class.to_s.downcase) , :method => "post"}
  = token_tag
  - @questionnaire.class.questions.each do |question|
    = render :partial => "question", :object => question, :locals => { :id => question.key }
  
  = submit_tag
