%h1 
  Answered 
  = @questionnaire.class.to_s.demodulize.upcase

%table
  - @questionnaire.class.questions.each do |question|
    %tr
      %td= question.key
      %td= question.title
      %td
        - case question.type
        - when :open
          = @questionnaire.send(question.key)
        - when :radio 
          = question.options[ @questionnaire.send(question.key).to_i ]
        - else
          Unknown question type
          = question.type.inspect

